#include <stdio.h>

int main()
{
  int array[100], N, i, j, temp;

  printf("Enter number of elements\n");
  scanf("%d", &N);

  printf("Enter %d integers\n", N);

  for (i = 0; i < N; i++)
    scanf("%d", &array[i]);

  for (i = 0 ; i < N - 1; i++)
  {
    for (j = 0 ; j < N - i - 1; j++)
    {
      if (array[j] > array[j+1]) /* For decreasing order use < */
      {
        temp = array[j];
        array[j] = array[j+1];
        array[j+1] = temp;
      }
    }
  }

  printf("Sorted list in ascending order:\n");

  for (i = 0; i < N; i++)
     printf("%d\n", array[i]);

  return 0;
}
